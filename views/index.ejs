<%- include ('partials/header.ejs') %>

<div class="music-container">
    <% for (let i = 0; i < musicas.length; i += 4) { %>
      <div class="music-row">
        <% for (let j = i; j < i + 4 && j < musicas.length; j++) { %>
          <div class="music-item">
            <h3><%= musicas[j].title %></h3>
            <p><%= musicas[j].year %></p>
          </div>
        <% } %>
      </div>
    <% } %>
  </div>

  <%- include ("partials/footer.ejs") %>